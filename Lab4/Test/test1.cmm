int hanoi(int count, int pole1, int pole2, int pole3) {
	write(count);
	write(pole1);
	write(pole2);
	write(pole3);
	if (count == 1){
		write(pole1*100000000+pole3);
	} else {
		hanoi(count-1,pole1, pole3, pole2);
		write(pole1*100000000+pole3);
		hanoi(count-1,pole2, pole1, pole3);
	}
	return 0;
}

int main() {
	int n;
	n = read();
	hanoi(n, 1, 2, 3);
	return 0;
}